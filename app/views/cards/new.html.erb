	<%= form_for @card, url: {action: 'create'}, html: { class: "form-group" }  do |form| %>

			 <div class="form-inline">
					 <label>Name:</label>
					 <%= form.text_field :name, class: 'form-control' %>
				 </div>
			 <div class="form-inline">
				 <label>Rarity:</label>
				 <%= form.text_field :rarity, class: 'form-control' %>
			 </div>
			 <div class="form-inline">
				 <label>Class:</label>
				 <%= form.text_field :klass, class: 'form-control' %>
			 </div>
			 <div class="form-inline">
				 <label>Kingdom:</label>
				 <%= form.text_field :kingdom, class: 'form-control' %>
			 </div>
			 <div class="form-inline">
				 <label>Collection:</label>
				 <%= form.text_field :collection, class: 'form-control' %>
			 </div>
			 <div class="form-inline">
				 <label>Collection number:</label>
				 <%= form.number_field :collection_number, class: 'form-control' %>
			 </div>
			 <div class="form-inline">
				 <label>Health points:</label>
				 <%= form.number_field :hp, class: 'form-control' %>
			 </div>
			 <div class="form-inline">
				 <label>Attack points:</label>
				 <%= form.number_field :attack, class: 'form-control' %>
			 </div>
			 <div class="form-inline">
				 <label>Skills:</label>
				 <%= form.text_field :skills, class: 'form-control' %>
			 </div>
			 <div class="form-inline">
				 <label>Image:</label>
				 <%= form.text_field :card_image, class: 'form-control' %>
			 </div>
		<%= form.submit "Create" %>
	<%end%>
